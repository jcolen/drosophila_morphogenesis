#Font
font.size : 8
font.family : Arial

#Axes
axes.titlesize : 8
axes.linewidth : 1.
axes.edgecolor : black

#Ticks
xtick.major.width : 1.
ytick.major.width : 1.

#Legend
legend.framealpha : 0.
legend.handlelength : 1.
legend.fontsize : 7

#Hatch
hatch.linewidth : 8

#Figure
figure.dpi : 300
figure.frameon : False

#Savefig
savefig.bbox : tight
savefig.format : svg
